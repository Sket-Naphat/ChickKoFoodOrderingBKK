rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /menu/{menuItem} {
      allow read: if true;
      allow write: if true;
    }
    match /orders/{orderId} {
      allow read, write: if true;
    }
    match /cost/{costId} {
      allow read, write: if true;
    }
    match /workTimeRecord/{workTimeRecordId} {
      allow read, write: if true;
    }
     match /stock/{stockId} {
      allow read, write: if true;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
